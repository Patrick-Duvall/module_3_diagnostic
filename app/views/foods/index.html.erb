<p><%= @food_presenter.count %> results</p>
<%  @food_presenter.foods.each do |food| %>
<section class='food-item'>
  <p class='food-item-name'><%= food.name %></p>
  <p class='food-item-manufacturer'><%= food.manufacturer %></p>
  <p class='food-item-ndb'><%= food.ndb %></p>
  <p class='food-item-group'><%= food.group %></p>
  <p class='food-item-data_source'><%= food.data_source %></p>
</section>

<% end %>
